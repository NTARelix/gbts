{
  "name": "gbjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "parcel build src/index.html",
    "deploy": "gh-pages -d dist",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "start": "parcel src/index.html",
    "test": "jest --watch"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "ts-jest": "^25.2.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/*.test.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "\\.test\\.tsx?$",
    "testURL": "http://localhost/",
    "transform": {
      "\\.tsx?$": "ts-jest"
    }
  }
}

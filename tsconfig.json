{
    "compilerOptions": {
        "allowImportingTsExtensions": true,
        "jsx": "react-jsx",
        "lib": [
            "dom",
            "es2015",
            "es2016",
            "es2017",
            "es2018"
        ],
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "noImplicitAny": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "ES2021"
    }
}
